{
  "name": "press",
  "version": "0.1.0",
  "description": "A Motoko MCP server generated by create-motoko-mcp-server for the Prometheus Protocol.",
  "scripts": {
    "start": "dfx start --background",
    "stop": "dfx stop",
    "deploy": "dfx deploy",
    "deploy:ic": "dfx deploy --network ic",
    "mops:install": "mops install",
    "inspector": "mcp-inspector",
    "test": "vitest run",
    "test:watch": "vitest",
    "app-store": "app-store-cli",
    "auth": "auth-cli"
  },
  "devDependencies": {
    "@dfinity/pic": "^0.16.1",
    "@icp-sdk/core": "^4.2.2",
    "@modelcontextprotocol/inspector": "^0.17.2",
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "motoko",
    "mcp",
    "prometheus-protocol",
    "dfinity",
    "internet-computer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@icp-sdk/canisters": "^2.0.1",
    "pem-file": "^1.0.1",
    "zod": "^4.2.1"
  },
  "pnpm": {
    "overrides": {
      "@dfinity/agent": "3.4.3",
      "@dfinity/auth-client": "3.4.3",
      "@dfinity/candid": "3.4.3",
      "@dfinity/principal": "3.4.3",
      "@dfinity/identity": "3.4.3"
    },
    "onlyBuiltDependencies": [
      "@dfinity/pic",
      "core-js",
      "esbuild",
      "fsevents"
    ]
  }
}
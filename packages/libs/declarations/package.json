{
  "name": "@press/declarations",
  "version": "0.0.0-development",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/generated/*",
      "types": "./dist/generated/*"
    }
  },
  "scripts": {
    "build": "npm-run-all clean compile copy:generated",
    "clean": "rimraf dist",
    "compile": "tsc",
    "copy:generated": "cpx \"src/generated/**/*\" \"dist/generated\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dfinity/agent": "^3.4.3",
    "@dfinity/candid": "^3.4.3",
    "@dfinity/principal": "^3.4.3",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  }
}
